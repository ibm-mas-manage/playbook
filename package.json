{
  "name": "manage-playbook",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.ibm.com/manage/playbook"
  },
  "bugs": {
    "url": "https://github.ibm.com/manage/playbook"
  },
  "scripts": {
    "build": "gatsby build",
    "build:prod": "gatsby build --prefix-paths",
    "clean": "gatsby clean",
    "dev": "gatsby develop",
    "dev-docker": "gatsby develop -H 0.0.0.0",
    "dev-docker-clean": "gatsby clean && gatsby develop -H 0.0.0.0",
    "dev:clean": "gatsby clean && gatsby develop",
    "serve": "gatsby serve",
    "serve-docker": "gatsby serve  -H 0.0.0.0",
    "start": "yarn dev",
    "start-docker": "yarn dev-docker",
    "start-docker-clean": "yarn dev-docker-clean",
    "test": "yarn format:diff",
    "format": "prettier --write \"**/*.{scss,css,js,md,mdx}\"",
    "format:diff": "prettier --check \"**/*.{scss,css,js,md,mdx}\""
  },
  "dependencies": {
    "carbon-components-react": "^7.22.0",
    "gatsby": "^2.24.5",
    "gatsby-theme-carbon": "^1.26.9",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "yarn": "^1.22.4"
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react-helmet": "^6.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true
  }
}
